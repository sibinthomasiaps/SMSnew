<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script>


    $(document).ready(function () {
        $("#getClass").change(function () {
            // $("#Ddldivision").append('<option></option>').val('').text('-Please select-');

            $.get("/Debitcard/GetDivisionList", { getClass: $("#getClass").val() }, function (data1) {
                $("#Ddldivision").empty();
                $("#Ddldivision").append("<option >" + "please select" + "</option>")
                $.each(data1, function (index, row) {
                    $("#Ddldivision").append("<option value ='" + row.ClassRoomDivision + "'>" + row.ClassRoomDivision + "</option>")
                });
            });
        })
    });
</script>




