@model SMSnew.Models.MenuSubMenu
 @{
    ViewBag.Title = "CreateSubMenu";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}

@using (Html.BeginForm())
{
    <!-- Container fluid  -->
    <!-- ============================================================== -->
    <div class="container-fluid">
        <!-- ============================================================== -->
        <!-- Start Page Content -->
        <!-- ============================================================== -->
        <!-- Row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="card card-outline-info">
                    <div class="card-header">
                        <h4 class="m-b-0 text-white">SubMenu</h4>
                    </div>
                    <div class="card-body">
                        <form action="#">
                            <div class="form-body">
                                <h3 class="card-title">Create SubMenu</h3>
                                <hr>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="SubjectClass">Main Menu<span class="danger"></span> </label>
                                            @Html.DropDownListFor(model => model.MenuId, ViewBag.menuname as SelectList, "---Select Menu---", new { @class = "form-control", @required = "required" })
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="SubjectName">Submenu Name<span class="danger"></span> </label>
                                            <input type="text" class="form-control" id="SubMenuName" name="SubMenuName" required>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="SubjectClass">Controller<span class="danger"></span> </label>
                                            <input type="text" class="form-control" id="SubMenuControllerName" name="SubMenuControllerName" required>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="SubjectName">SubMenu Path<span class="danger"></span> </label>
                                            <input type="text" class="form-control" id="SubMenuPath" name="SubMenuPath" required>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-actions">
                                    <button class="btn btn-info waves-effect waves-light" type="submit" id="submit" name="submit" style="background-color: #0c4e90;border: 1px solid #0c4e90;color:#ffffff;"><span class="btn-label"><i class="fa fa-check"></i></span>Save</button>
                                    <button class="btn btn-info waves-effect waves-light" type="button" id="cancel" name="cancel" onclick="document.location.reload(true)" style="background-color:#2f3d4a;border: 1px solid #2f3d4a;color:#ffffff;"><span class="btn-label"><i class="fa fa-close"></i></span>Cancel</button>
                                    <button class="btn btn-info waves-effect waves-light" type="button" style="background-color:#dc3545;border: 1px solid #dc3545;color:#ffffff;"><span class="btn-label"><i class="fa fa-list"></i></span><a href="DetailsSubjectList" style="color:#ffffff">Back to List</a></button>
                                    @*<button type="submit" class="btn btn-success"> <i class="fa fa-check"></i> Save</button>
                                        <button type="button" class="btn btn-inverse" onclick="javascript:document.location.href='@Url.Action("CreateSubject", "Subject")'">Cancel</button>*@
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
       

        <!-- ============================================================== -->
        <!-- End PAge Content -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Right sidebar -->
        <!-- ============================================================== -->
