@model IEnumerable<SMSnew.Models.Circular>

<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Data Export</h4>
                <h6 class="card-subtitle">Export data to Copy, CSV, Excel, PDF & Print</h6>
                <div class="table-responsive m-t-40">
                    <table id="myTable" class="table table-bordered table-striped">
                        <thead>
                            @*<tr><td>@Html.ActionLink("Create Circular", "CreateCircular")</tr>*@
                            <tr>
                                <th>CircularNo</th>
                                <th>CircularSubject</th>
                                <th>IssueDate</th>
                                <th>Description</th>
                            </tr>
                        </thead>
                        <tfoot>

                        </tfoot>
                        <tbody>
                            @foreach (var item in Model)
                            {


                                <tr>

                                    <td>@Html.DisplayFor(modelItem => item.CirNumber)</td>
                                    <td>@Html.DisplayFor(modelItem => item.CirSubject)</td>
                                    <td>@Html.DisplayFor(modelItem => item.CirIssueDate, new { type = "date" })</td>
                                    <td>@Html.DisplayFor(modelItem => item.CirDescription)</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

